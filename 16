using System;
class Program
{
    static void Main()
    {
        Console.Write("Введите массу в килограммах: ");
        int kilograms = Convert.ToInt32(Console.ReadLine());
        int centners = kilograms / 100;
        Console.WriteLine($"Число полных центнеров в {kilograms} килограммах: {centners}");
    }
}
